Contact
{
	label: "Edit Contact";
	actions: save, cancel;
	props: -id,
			action.save{labelImage: "resources/images/toolbarButtonGraphics/general/Save16.gif"},
			action.cancel{labelImage: "/resources/images/toolbarButtonGraphics/general/Undo16.gif"},
			firstName, lastName, phoneNumber, 
			category{fieldType: wicket.contrib.webbeans.databinder.examples.CategoryField};
}
Category
{
	label: "Edit Category";
	actions: save, cancel;
	props: -id, name;
}

